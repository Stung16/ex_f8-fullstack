<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css"
      integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm"
      crossorigin="anonymous"
    />
    <title>history email</title>
  </head>
  <body>
    <h2>Lịch sử gửi mail</h2>
    <a href="/" class="btn btn-info text-white">Trở lại</a>
    <ul class="list-group">
        <li class="list-group-item">
        <% emails.forEach(email => { %>
          <div><span>Gửi đến: <%= email.to %></span></div>
          <div>
            <span
              >Thời gian gửi: <%=
              moment(email.created_at).format('HH:mm DD-MM-YYYY ') %></span
            >
          </div>
          <div>
            <span
              >TIêu đề: <%=
              email.title
              %></span
            >
          </div>
          <div>
            <span
              >Nội dung: <%=
              email.message
              %></span
            >
          </div>
        <hr />
        <% }) %>
      </li>
    </ul>
  </body>
</html>
